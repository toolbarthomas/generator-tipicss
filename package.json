{
  "name": "generator-tipicss",
  "version": "0.6.0",
  "description": "Yeoman Generator for Tipicss",
  "homepage": "https://github.com/toolbarthomas/generator-tipicss",
  "author": {
    "name": "Thomas van der Velde",
    "email": "contact@toolbarthomas.net",
    "url": "http://toolbarthomas.net"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "tipicss generator yeoman",
    "yeoman-generator"
  ],
  "devDependencies": {
    "yeoman-test": "^1.7.0",
    "yeoman-assert": "^3.1.0",
    "nsp": "^3.1.0",
    "eslint": "^4.1.0",
    "prettier": "^1.7.0",
    "lint-staged": "^6.1.0",
    "eslint-config-prettier": "^2.4.0",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-config-xo": "^0.19.0",
    "jest": "^22.1.4",
    "jest-cli": "^22.1.4"
  },
  "dependencies": {
    "yeoman-generator": "^2.0.1",
    "del": "^3.0.0",
    "dotenv": "^5.0.0",
    "download-git-repo": "^1.0.1",
    "fs-extra": "^5.0.0",
    "path": "^0.12.7",
    "replace-in-file": "^3.0.0",
    "update-notifier": "^2.3.0",
    "chalk": "^2.1.0",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "prepublish": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": "https://toolbarthomas@github.com/toolbarthomas/generator-tipicss",
  "license": "MIT"
}
